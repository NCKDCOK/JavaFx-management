<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="440.0" prefWidth="377.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.LoginController">
   <children>
      <Pane layoutX="18.0" layoutY="14.0" prefHeight="448.0" prefWidth="341.0">
         <children>
            <Text layoutX="81.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="欢迎登录">
               <font>
                  <Font size="45.0" />
               </font></Text>
            <TextField fx:id="username" focusTraversable="false" layoutX="70.0" layoutY="169.0" prefHeight="43.0" prefWidth="201.0" promptText="用户名" />
            <Button fx:id="login" focusTraversable="false" layoutX="143.0" layoutY="304.0" mnemonicParsing="false" onAction="#login" prefHeight="21.0" prefWidth="56.0" text="登录" />
            <PasswordField fx:id="password" focusTraversable="false" layoutX="70.0" layoutY="232.0" prefHeight="43.0" prefWidth="201.0" promptText="密码" />
            <Hyperlink focusTraversable="false" layoutX="203.0" layoutY="275.0" text="忘记密码？" />
         </children>
      </Pane>
   </children>
</AnchorPane>
